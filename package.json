{
    "scripts": {
        "develop": "eleventy --serve",
        "build": "eleventy",
        "postbuild": "npm run postbuild:js && npm run postbuild:font && npm run postbuild:svg",
        "postbuild:js": "uglifyjs _site/js/app.js -o _site/js/app.js",
        "postbuild:font": "subfont _site/index.html --in-place --inline-css --no-fallbacks",
        "postbuild:svg": "svgo _site/svg/*",
        "serve": "http-server -p 8000 _site",
        "lighthouse": "npm run build && lhci autorun"
    },
    "dependencies": {},
    "devDependencies": {
        "@11ty/eleventy": "^0.11.1",
        "@11ty/eleventy-cache-assets": "^2.1.0",
        "@11ty/eleventy-img": "^0.8.0",
        "@lhci/cli": "^0.7.0",
        "@octokit/rest": "^18.1.1",
        "dotenv": "^8.2.0",
        "html-minifier": "^4.0.0",
        "http-server": "^0.12.3",
        "subfont": "^5.3.0",
        "svgo": "^1.3.2",
        "uglify-js": "^3.12.8"
    }
}
